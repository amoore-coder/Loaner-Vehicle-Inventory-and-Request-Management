        <script><![CDATA[(function executeRule(current, previous /*null when async*/){

	var track = current.vehicle_status;
	var gr = new GlideRecord('x_1394653_lva_loaner_vehicle_catalog'); //var ga = new gliderecord('x_1394653_lva_vehicle_tracker)
	
	
	gr.addQuery('sys_id',current.vehicle);
	gr.query();
	if (gr.next()){

	if(track == 'Pending Release'|| track == 'Out on Field' || track == 'Returned for Inspection'|| track == 'Sent for Servicing'){

		gr.status = 'unavailable';

	}
	else
	if(track == 'Returned to Warehouse'){
		gr.status = 'available';
	}
	else
	if(track =='Decommissioned'){
	gr.status = 'decommissioned';
	}

	gr.update();
	}
})(current, previous);]]></script>
